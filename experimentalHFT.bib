@article{Bershova2012,
abstract = {With the proliferation of high-frequency trading (HFT), understanding the effects of HFT on market quality and the opportunities that HFT creates for long-term (LT) investors is important in building an efficient regulatory framework. This paper demonstrates an approach that allows us to estimate the effects of HFT on market quality using information on daily aggregate volumes of HFT and of LT investors provided by a bulge-bracket broker in two trading environments that are different in terms of electronic liquidity, the evolving Tokyo equity market and the mature London equity market. Our results suggest that HFT is mostly involved in opportunistic liquidity provisioning rather than engaging in predatory strategies, at least for liquid stocks. While HFT market making activity increases short-term intraday volatility and therefore adversely affects transaction costs, this effect is more than offset by the significant compression of bid-ask spreads leading to a net reduction of trading costs for LT investors.},
